	<script type="text/javascript">
		jumsu = 78;
		if(jumsu<0 || jumsu>100){
			alert("잘못입력!!");	
		} else{
			switch(parseInt(jumsu/10)){
			/* switch(Math.floor(jumsu/10)){ //이것도 사용 가능하다.*/
				case 10:
				case 9:
					document.write("<h1>환영합니다.(수)</h1>");
					document.write("<img src='../images/jang1.jpg'/>");
					break;
				case 8:
					document.write("우");
					break;
				case 7:
					document.write("미");
					break;
				case 6:
					document.write("양");
					break;
				default:
					document.write("가");	
			}
		}
	</script>

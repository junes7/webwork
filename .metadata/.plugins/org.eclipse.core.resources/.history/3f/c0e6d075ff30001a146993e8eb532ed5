<body>
	<script type="text/javascript">
		num = 101;
		if(num<0 && num>100){
			alert("잘못입력!!");	
		} else{
			switch(num/10){
				case 10:
				case 9:
					document.write("<h1>환영합니다.(A)</h1>");
					document.write("<img src='../images/jang1.jpg'/>");
				case 8:
					document.write("우");
				case 7:
					document.write("미");
				case 6:
					document.write("양");
				default:
					document.write("가");	
			}
		}
	
	
	
	</script>
</body>
</html>
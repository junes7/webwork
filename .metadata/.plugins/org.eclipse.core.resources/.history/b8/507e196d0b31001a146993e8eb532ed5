<body>
	<script type="text/javascript">
		document.write("<h1>구구단표</h1>")
		document.write("<table border=1 >");
			for(var i = 1; i < 10; i++){
				document.write("<tr>");
				for(var j = 1; j < 10; j++){
					if(i=j){
						document.write("<td bgcolor=yellow>" + i + "*" +j + "=" + i*j +"</td>");
					}
				}
				document.write("</tr>")
			}
		document.write("</write>");
	</script>
</body>

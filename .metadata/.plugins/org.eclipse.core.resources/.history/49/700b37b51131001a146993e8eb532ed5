		function printGuGu(i){
			document.write("<h1>구구단표</h1>")
			document.write("<table border=1 >");
				for(var i = 1; i < 10; i++){
					document.write("<tr>");
					for(var j = 1; j < 10; j++){
						document.write( "<td>" +i + "*" +j + "=" + i*j +"</td>");
					}
					document.write("</tr>")
				}
			document.write("</table>");			
		}	
	</script>
	<script type="text/javascript">
		function calcNum(num){
			sum=0;
			sum+=num;
			return sum;
		}	
	</script>

		function printGuGu(i){
			document.write("<table border=1 >");
			for(var j=1; j<10; i++){
				document.write("<tr>");
				document.write("<td>" +i + "*" +j + "=" + i*j + "</td>");
				document.write("</tr>");
				}
			document.write("</table>");			
		}	
	</script>
